pipeline {
    agent { docker { image 'python:3.5.1' } }
    stages {
        stage('build') {
            steps {
                sh 'python --version'
            }
        }
        stage('STAGE 00'){
            steps{
                echo "Pipeline Usando Jenkinsfile"
            }
        }
        stage('STAGE 01'){
            steps{
                echo "Pipeline Usando Jenkinsfile"
            }
        }
    }
}
